<h1><strong>All stations</strong></h1>
<p id = "click_info">Click any station below to see it on a map</p>

<section id = "station_list">
  <%= will_paginate @stations %>

  <% for station in @stations %>
    <%= link_to station, station_info_path(:data => station) %><br>
  <% end %>

  <%= will_paginate @stations %>
</section><!--station_list!-->