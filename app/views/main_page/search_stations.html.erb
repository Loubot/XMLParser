    <p id="searchHeader" class="navbar-text">Where's My Train</p>
    <%= content_tag :h2, id:"all_trains_link", class: "navbar-right" do %>
      <%= link_to 'All trains', all_trains_path %>
    <% end %>
  </nav>
</div>
<section id ="searchStations">
	<%= form_for search_results_path, :url => search_results_path, html: { class: "searchForm" },:method => 'get' do |f| %>
